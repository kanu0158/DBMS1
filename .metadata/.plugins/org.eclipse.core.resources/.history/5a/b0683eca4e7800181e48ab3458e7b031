-- account
-- money(decimal),  account_seq(decial), account_num, 
-- account_type, create_date, limit
CREATE TABLE account(
	account_seq DECIMAL PRIMARY KEY,
	account_num VARCHAR2(20),
	account_type VARCHAR2(20),
	create_date DATE,
	money DECIMAL,
	limit DECIMAL
);
DROP TABLE account;

