package domain;

public class StaffBean extends MemberBean {
	private String accessNum;

	
	public String getAccessNum() {
		return accessNum;
	}


	public void setAccessNum(String accessNum) {
		this.accessNum = accessNum;
	}


	@Override
	public String toString() {
		return "회원정보 [아이디:" + uid 
				+ ", 비번:" + pass + ", 이름:" + name 
				+ ", 주민:" + ssn + ", 주소:" + addr
				+ ", 폰번호:" + phone + "]"
				+ "접근번호=" + accessNum + "]\n";
	}
}
